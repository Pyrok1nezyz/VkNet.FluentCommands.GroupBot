<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.1;netstandard2.0;net47</TargetFrameworks>
        <Version>1.3.0</Version>
        <Description>
            Extension for VkNet to quickly create bots.
        </Description>
        <Authors>ariusbronte</Authors>
        <RepositoryType>GIT</RepositoryType>
        <RepositoryUrl>https://github.com/ariusbronte/VkNet.FluentCommands.GroupBot</RepositoryUrl>
        <PackageId>VkNet.FluentCommands.GroupBot</PackageId>
        <PackageTags>vknet;vk;api;bot;group</PackageTags>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="VkNet" Version="1.49.0" />
    </ItemGroup>
</Project>
